    <script type="text/html" data-name="simple">
        <div class="col pl-0">
            {{#title}}
            <a href="{{url}}" class="fs-16 d-block font-weight-bold mb-2 " title="{{title}}">
                {{#helpers.snippet}}{"attribute": "title"}{{/helpers.snippet}}
            </a>
            {{/title}}

            {{#date}}
            <p class="text-muted mb-3">
                {{date}}
            </p>
            {{/date}}

            {{#category}}
            <div class=" mb-2 ">
                <a href="{{url}}" title="{{name}}" class="border-bottom border-primary text-muted">{{name}}</a>
            </div>
            {{/category}}

            <a href="{{url}}" title="{{title}}">
                {{#summary}}
                {{#helpers.snippet}}{"attribute": "summary"}{{/helpers.snippet}}
                {{/summary}}
                {{^summary}}
                {{#helpers.snippet}}{"attribute": "content"}{{/helpers.snippet}}
                {{/summary}}
            </a>
            <div class=" mt-2 ">
                {{#_tags}}
                <span class="badge badge-light m-1">{{name}}</span>
                {{/_tags}}
            </div>

            <div class=" mt-2 ">
                {{#rating}}
                {{{rating}}}
                {{/rating}}
            </div>
        </div>
    </script>
    <script type="text/html" data-name="owner">
        {{#owner}}
        <div class="row mt-3" data-id="{{id}}">
            {{#image}}
            <div class="col-2 col-sm-3">
                <img src="{{image}}" alt="{{ name }}" class="rounded-circle">
            </div>
            {{/image}}
            <div class=" col small">
                <p>{{name}}</p>
                <p class="mt-3  text-muted">{{headline}}</p>
            </div>
        </div>
        {{/owner}}
    </script>
    <script type="text/html" data-name="default">
        <article class="h-100 d-flex " data-id="{{id}}">
            <a href="{{url}}" class="col-3 col-sm-2 pl-0">
                {{#image}}
                <img src="{{image}}" alt="{{ title }}" class="rounded">
                {{/image}}
            </a>
            #includeTemplate:simple#
        </article>
    </script>
    <script type="text/html" data-name="default2">
        <a class=" mb-2 d-block" href="{{url}}" title="{{title}}">
            {{#image}}
            <figure class="mb-6">
                <img src="{{image}}" alt="{{ title }}" class="rounded ">
            </figure>
            {{/image}}

            {{#title}}
            <p class="fs-16 font-weight-bold mb-3 ">
                {{#helpers.snippet}}{"attribute": "title"}{{/helpers.snippet}}
            </p>
            {{/title}}
            {{#date}}
            <p class="text-muted mb-3">
                {{date}}
            </p>
            {{/date}}
            <p>
                {{#summary}}
                {{#helpers.snippet}}{"attribute": "summary"}{{/helpers.snippet}}
                {{/summary}}
            </p>
        </a>
    </script>


    <script type="text/html" data-name="article">
        <article class="h-100 d-flex flex-column" data-id="{{id}}">
            #includeTemplate:default2#
            #includeTemplate:owner#
        </article>
    </script>
    <script type="text/html" data-name="webinar">
        <article class="h-100 d-flex flex-column" data-id="{{id}}">
            #includeTemplate:default2#
            {{#owner}}
            <div class="row mt-3" data-id="{{id}}">
                {{#image}}
                <div class="col-2 col-sm-3">
                    <img src="{{image}}" alt="{{ name }}" class="">
                </div>
                {{/image}}
                <div class=" col small">
                    <p class="mb-2  text-muted"> Powered By</p>
                    <p>{{name}}</p>
                </div>
            </div>
            {{/owner}}
        </article>
    </script>
    <script type="text/html" data-name="job">
        <article class="h-100 d-flex " data-id="{{id}}">
            #includeTemplate:simple#
        </article>
    </script>

    <script type="text/html" data-name="event"
              data-use-template="job">
